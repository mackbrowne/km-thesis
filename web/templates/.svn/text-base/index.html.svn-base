<html>
  <head>
    <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/themes/smoothness/jquery-ui.css" type="text/css" media="all" />
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.min.js"></script>
    <link rel="stylesheet" href="{{url_for('static', filename='mystyle.css')}}"/>
    <link rel="stylesheet" href="static/bootstrap.min.css"/>
  </head>

  <body>
  {% for (group, cams) in locs.items() %}
  <h1 class="group">{{group|upper}}</h1>
    <div class="group">
      {% for loc in cams %}
      <a class="btn primary" 
         href="{{url_for('movie', group=group, locid=loc.id)}}">
         {{loc.name}}
      </a>
      {% endfor %}
    </div>
  {% endfor %}
  </body>

</html>
