{% extends "layout.html" %}
{% block body %}


  {% for (group, cams) in locs.items() %}
  <h1 class="group">{{group|upper}}</h1>
    <div class="group">
      <ul>
      {% for loc in cams %}
      <li>
      {{loc.id}} --
      <a href="{{url_for('movielayout', group=group, locid=loc.id)}}">
         {{loc.name}}
      </a>
      </li>
      {% endfor %}
      </ul>
      
    </div>
  {% endfor %}
  
{% endblock %}

  